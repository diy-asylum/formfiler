{
  "$id": "SpouseInfo.json",
  "$schema":"I589ApplicationV1",
  "title": "Spouse Info",
  "description": "Personal information about the applicant's spouse.",
  "type": "object",
  "properties": {
    "lastName": {
      "type": "string"
    },
    "firstName": {
      "type": "string"
    },
    "middleName": {
      "type":"string"
    },
    "aliases": {
      "type":"array",
      "items": {
        "type":"string"
      }
    },
    "dateOfBirth":{
      "type":"string",
      "format":"mm/dd/yyyy"
    },
    "alienRegistrationNumber":{
      "type":"string"
    },
    "socialSecurityNumber":{
      "type":"string"
    },
    "passportNumber":{
      "type":"string"
    },
    "dateOfMarriage":{
      "type":"string",
      "format":"mm/dd/yyyy"
    },
    "placeOfMarriage":{
      "type":"string"
    },
    "cityOfBirth": {
      "type":"string"
    },
    "countryOfBirth":{
      "type":"string"
    },
    "nationality":{
      "type":"string"
    },
    "raceEthnicOrTribalGroup": {
      "type":"string"
    },
    "gender": {
      "type":"string",
      "enum":["MALE","FEMALE"]
    },
    "inUS":{
      "type":"boolean"
    },
    "locationInUS":{
      "type":"string"
    },
    "placeOfLastEntry":{
      "type":"string"
    },
    "dateOfLastEntry":{
      "type":"string",
      "format":"mm/dd/yyyy"
    },
    "i94Number":{
      "type":"string"
    },
    "immigrationStatusWhenLastAdmitted":{
      "type":"string"
    },
    "currentImmigrationStatus":{
      "type":"string"
    },
    "statusExpirationDate":{
      "type":"string",
      "format":"mm/dd/yyyy"
    },
    "isInImmigrationCourt":{
      "type":"boolean"
    },
    "previousArrivalDate":{
      "type":"string",
      "format":"mm/dd/yyyy"
    },
    "includeInApplication":{
      "type":"boolean"
    }
  },
  "required":["all"]
}

